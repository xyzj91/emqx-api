{
  "name": "hyperf/emqx-api",
  "description": "EMQX Management HTTP API",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "psr/container": "^1.0|^2.0",
    "guzzlehttp/guzzle": "^6.3|^7.0",
    "hyperf/guzzle": "^2.0",
    "hyperf/di": "^2.0",
    "hyperf/utils": "^2.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.0",
    "php-coveralls/php-coveralls": "^2.0",
    "phpunit/php-code-coverage": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "EMQX\\API\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
